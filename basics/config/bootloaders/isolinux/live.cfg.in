label live-@FLAVOUR@-persistence
menu label ^Live (@FLAVOUR@ persistence)
menu default
linux @LINUX@ persistence
initrd @INITRD@
append @APPEND_LIVE@

label live-@FLAVOUR@
	menu label ^Live (@FLAVOUR@)
	linux @LINUX@
	initrd @INITRD@
	append @APPEND_LIVE@

label live-@FLAVOUR@-failsafe
	menu label ^Live (@FLAVOUR@ failsafe)
	linux @LINUX@
	initrd @INITRD@
	append @APPEND_LIVE_FAILSAFE@
